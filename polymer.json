{
  "entrypoint": "index.html",
  "shell": "src/joornalo.html",
  "fragments": [
    "src/elements/page-front.html",
    "src/elements/page-news.html",
    "src/elements/page-galleries.html",
    "src/elements/page-gallery.html",
    "src/elements/page-search.html",
    "src/elements/page-multimedia.html",
    "src/elements/page-notfound.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "mocks/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "bundle": true,
    "addServiceWorker": true,
    "js": {
      "minify": true,
      "compile": true
    },
    "css": {
      "minify": true
    },
    "html": {
      "minify": true
    }
  }],
  "buildsAlt": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
