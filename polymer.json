{
  "entrypoint": "index.html",
  "shell": "src/joornalo.html",
  "fragments": [
    "src/elements/page-front.html",
    "src/elements/page-news.html",
    "src/elements/page-galleries.html",
    "src/elements/page-search.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "mocks/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "bundle": false,
    "js": {
      "minify": false,
      "compile": false
    },
    "css": {
      "minify": false
    },
    "html": {
      "minify": false
    }
  }],
  "buildsAlt": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
